{% include 'menu.html.twig' %}
<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8"/>
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		{% block stylesheets %}
							<style>
		body {
    		height: 100%;
		}

		html {
    		display: table;
    		margin: auto;
		}

		body {
			font-family: Arial, Helvetica, sans-serif;
    		display: table-cell;
    		vertical-align: middle;
		}
		table{
			margin: 10px;
			text-align: center;
			background-color: cyan;
		}
		th{
			background-color: white;
		}
		tr{
			background-color: white;
		}
			body{
				background-color: cyan;
			}
			th{
				border:3px solid black;
			}
			td{
				border:3px solid black;
			    padding: 28px;
			}
			tr:hover {
				background-color: cyan;
			}
			ul {
				position: sticky;
				list-style-type: none;
				margin: 0;
				padding: 0;
				overflow: hidden;
			}

			li {
				border-right: 2px solid #FF5F9E;
  				float: right;
			}

			li a {
  			display: block;
 			color: Black;
  			text-align: center;
  			padding: 14px 16px;
  			text-decoration: none;
			}
			a{
				padding: 14px 16px;
			}
			h1{
				text-align: center;
				color: Black;	
				margin: 0px;
			}
			</style>
		{% endblock %}
	</head>
	<body>
		{% block body %}{% endblock %}
		{% block javascripts %}{% endblock %}
	</body>
</html>
