{% include 'menu.html.twig' %}
<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8"/>
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		{% block stylesheets %}
		<style>
		body {
    		height: 100%;
		}

		html {
    		display: table;
    		margin: auto;
		}

		body {
			font-family: Arial, Helvetica, sans-serif;
    		display: table-cell;
    		vertical-align: middle;
		}
		table{
			margin: 10px;
			text-align: center;
			background-color: black;
		}
		th{
			background-color: white;
		}
		td{
			background-color: white;
		}
			</style>
		{% endblock %}
	</head>
	<body>
		{% block body %}{% endblock %}
		{% block javascripts %}{% endblock %}
	</body>
</html>
